{
	"name": "vscode-alt-underscore",
	"displayName": "Alternate Underscore",
	"description": "Replaces the character used for underscore in text editors",
	"version": "0.0.1",
	"publisher": "devscodev",
	"homepage": "https://github.com/devscodev/vscode-alt-underscore",
	"bugs": "https://github.com/devscodev/vscode-alt-underscore/issues",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/devscodev/vscode-alt-underscore"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Visualization",
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Alt Underscore",
			"properties": {
				"altUnderscore": {
					"type": "string",
					"scope": "application",
					"description": "Character to use in place of underscore",
					"default": ":",
					"examples": [":", "ː", "ˑ", "·"]
				}
			}
		}
	},
	"devDependencies": {
		"@types/vscode": "1.73.0",
		"@types/node": "^18.11.18",
		"typescript": "4.9.4"
	}
}